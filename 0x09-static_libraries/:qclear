#include "main.h"

/**
 * _memset - a function that fills memory with constant bytes
 * @s: locaion to fill
 * @b: char to fil location with
 * @n: number of bytes to fill
 * Return: returns pointer to locatio filled
 */

char *_memset(char *s, char b, unsigned int n)
{
	char *start = s;

	while (n--)
	{
		*s = b;
		s++;
	}
	return (start);
}
